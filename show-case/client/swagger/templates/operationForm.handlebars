{{#if requestBody}}
   {{methodName}}FormGroup(value?:{{{requestBody.type}}}) {
   {{#isRef requestBody.spec.schema }}
     let schema: any =  {{{requestBody.type}}}Schema;
   {{else}}
     let schema: any = {{{json requestBody.spec.schema}}}
   {{/isRef}}
